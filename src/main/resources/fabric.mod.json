{
  "schemaVersion": 1,
  "id": "jeromod",
  "version": "${version}",

  "name": "Jero Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Jero"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "net.jero.jeromod.JeroMod"
    ],
    "client": [
      "net.jero.jeromod.JeroModClient"
    ]
  },
  "mixins": [
    "jeromod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "fabric-key-binding-api-v1": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
